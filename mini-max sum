 public static void miniMaxSum(List<Integer> arr) {
    // Write your code here
    int n=arr.size();
    Collections.sort(arr);
     long min=arr.get(0);
     long max=arr.get(n-1);
     long sum=0;
        for(int i=0;i<n;i++) {
            sum +=arr.get(i);
            
        }
        long a=sum-max;
        long b=sum-min;
        System.out.println(a+ " " +b);
    }
    }
